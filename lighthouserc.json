{
  "ci": {
    "assert": {
      "preset": "lighthouse:recommended"
    },
	 "collect": {
      "url": [
        "$BASE_URL/",
		"$BASE_URL/brands/"
      ]
    },
    "upload": {
      "urlReplacementPatterns": [
        "s/[0-9a-f]{12}$/HASH/",
        "s#:[0-9]{3,5}/#:PORT/#",
        "s/[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}/UUID/ig"
      ]
    }
  }
}
